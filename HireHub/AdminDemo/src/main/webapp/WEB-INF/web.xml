<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
     <servlet>
     	<servlet-name>adminlogin</servlet-name>
     	<servlet-class>com.Admin.LoginServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>adminlogin</servlet-name>
     	<url-pattern>/adminlog</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>adminsearch</servlet-name>
     	<servlet-class>com.Admin.AdminSearchServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>adminsearch</servlet-name>
     	<url-pattern>/adminsearch</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>admininsert</servlet-name>
     	<servlet-class>com.Admin.AdminInsertServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>admininsert</servlet-name>
     	<url-pattern>/admininser</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>adminupdate</servlet-name>
     	<servlet-class>com.Admin.UpdateAdminServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>adminupdate</servlet-name>
     	<url-pattern>/adminupdate</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>deleteadmin</servlet-name>
     	<servlet-class>com.Admin.AdminDeleteServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>deleteadmin</servlet-name>
     	<url-pattern>/deleteadm</url-pattern>
     </servlet-mapping>
     
</web-app>